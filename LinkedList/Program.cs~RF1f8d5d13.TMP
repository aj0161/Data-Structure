    class Node
    {
        public Node next;
        public Item data;

    }
    class Item
    {
        private int zip = 0;
        public int Zip
        {
            get { return zip; }
            set { zip = value; }
        }
       

        private string address;
        public string Address
        {
            get { return address; }
            set { address = value; }
        }

    }

    class LinkedList
    {
        private Node head;

        public void AddFirst(Item data)
        {
            Node toAdd = new Node();
            toAdd.data = data;
            toAdd.next = head;

            head = toAdd;
        }
    }

   
